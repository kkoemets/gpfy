{
  "name": "config",
  "version": "1.0.0",
  "description": "Configuration scripts, managers",
  "main": "index.js",
  "type": "commonjs",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "pylint": "pylint src/com --rcfile .pylintrc",
    "run-client-tests": "PYTHONPATH=../src nosetests tests",
    "create-client-config-file": "node create-config-file.js --bot",
    "run-bot-client": "nohup python3 src/run_telegram_bot.py &",
    "run-bot-dev-client": "python3 src/run_telegram_bot.py",
    "docker-build-bot": "docker build -t gpfy ..",
    "docker-run-bot": "docker run -dp 3000:3000 gpfy",
    "docker-stop-bot": "docker ps -a -q  --filter ancestor=gpfy"
  },
  "author": "Kristjan Koemets",
  "license": "ISC",
  "devDependencies": {
    "python-shell": "^3.0.0"
  },
  "dependencies": {}
}
